{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "meowasite",
  "compatibility_date": "2025-04-03",
  "main": "./api/index.ts",
  "minify": true,
  "assets": {
    "not_found_handling": "404-page",
    "html_handling": "drop-trailing-slash",
    "run_worker_first": [
      "/api/*"
    ]
  },
  "placement": {
    "mode": "smart"
  }
}